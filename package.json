{
  "name": "resin-semver",
  "version": "1.6.0",
  "description": "Resin.io specific semver utility methods",
  "main": "src/index.js",
  "typings": "src/index.ts",
  "homepage": "https://github.com/resin-io-modules/resin-semver",
  "repository": {
    "type": "git",
    "url": "https://github.com/resin-io-modules/resin-semver"
  },
  "keywords": [
    "resin",
    "resin.io",
    "semver"
  ],
  "scripts": {
    "test": "npm run lint && ./automation/test.js",
    "build": "tsc && npm run docs",
    "lint": "tslint -t stylish -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "docs": "./automation/docs.js",
    "ci": "npm run build && catch-uncommitted && npm test",
    "precommit": "lint-staged",
    "prettify": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "author": "Lucian Buzzo <lucian@resin.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/lodash": "^4.1.2 <=4.14.121",
    "@types/semver": "^5.4.0",
    "lodash": "^4.1.2",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "catch-uncommitted": "^1.0.0",
    "chai": "^4.1.0",
    "husky": "^0.14.3",
    "jsdoc-to-markdown": "^3.0.0",
    "lint-staged": "^4.0.4",
    "mocha": "^3.4.2",
    "prettier": "^1.6.1",
    "ts-node": "^3.2.1",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "npm run lint",
      "npm run docs",
      "git add"
    ]
  }
}
