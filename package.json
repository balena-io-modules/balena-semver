{
  "name": "balena-semver",
  "version": "2.0.2",
  "description": "Balena specific semver utility methods",
  "main": "build/index.js",
  "typings": "src/index.ts",
  "homepage": "https://github.com/balena-io-modules/balena-semver",
  "repository": {
    "type": "git",
    "url": "https://github.com/balena-io-modules/balena-semver"
  },
  "files": [
    "build/",
    "src/"
  ],
  "keywords": [
    "balena",
    "balena.io",
    "resin",
    "resin.io",
    "semver"
  ],
  "scripts": {
    "test": "npm run build && npm run lint && node ./automation/test.js",
    "build": "tsc && npm run docs",
    "lint": "tslint -t stylish -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "docs": "node ./automation/docs.js",
    "ci": "npm run build && catch-uncommitted && npm test",
    "prettify": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "npm run build"
  },
  "author": "Lucian Buzzo <lucian@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/lodash": "^4.1.2 <=4.14.121",
    "@types/semver": "^5.4.0",
    "lodash": "^4.1.2",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "catch-uncommitted": "^1.3.0",
    "chai": "^4.2.0",
    "husky": "^2.3.0",
    "jsdoc-to-markdown": "^5.0.0",
    "lint-staged": "^8.1.7",
    "mocha": "^6.1.4",
    "prettier": "^1.17.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "npm run lint",
      "npm run docs",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
